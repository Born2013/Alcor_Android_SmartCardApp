<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_window_focused="false">
        <nine-patch android:src="@drawable/oldphone_dialog_textfield_default" />
    </item>
    <item android:state_enabled="false" android:state_window_focused="false">
        <nine-patch android:src="@drawable/oldphone_dialog_textfield_disabled" />
    </item>
    <item android:state_enabled="true" android:state_focused="true">
        <nine-patch android:src="@drawable/oldphone_dialog_textfield_focused" />
    </item>
    <item android:state_enabled="true">
        <nine-patch android:src="@drawable/oldphone_dialog_textfield_default" />
    </item>
    <item android:state_focused="true">
        <nine-patch android:src="@drawable/oldphone_dialog_textfield_default" />
    </item>
    <item>
        <nine-patch android:src="@drawable/oldphone_dialog_textfield_disabled" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/oldphone_dialog_textfield_pressed" />
    </item>
</selector>
